{
  "name": "SkyNft",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "account": "pnpm --filter @SkyNft/hardhat account",
    "chain": "pnpm --filter @SkyNft/hardhat chain",
    "fork": "pnpm --filter @SkyNft/hardhat fork",
    "deploy:contracts": "pnpm --filter @SkyNft/hardhat deploy:contracts",
    "verify": "pnpm --filter @SkyNft/hardhat verify",
    "compile": "pnpm --filter @SkyNft/hardhat compile",
    "generate": "pnpm --filter @SkyNft/hardhat generate",
    "hardhat:size": "pnpm --filter @SkyNft/hardhat size",
    "hardhat:lint": "pnpm --filter @SkyNft/hardhat lint",
    "hardhat:lint-staged": "pnpm --filter @SkyNft/hardhat lint-staged",
    "hardhat:test": "pnpm --filter @SkyNft/hardhat test --",
    "test": "pnpm run hardhat:test",
    "start": "pnpm --filter @SkyNft/nextjs dev",
    "next:lint": "pnpm --filter @SkyNft/nextjs lint",
    "next:format": "pnpm --filter @SkyNft/nextjs format",
    "next:check-types": "pnpm --filter @SkyNft/nextjs check-types",
    "postinstall": "husky install",
    "precommit": "lint-staged",
    "vercel": "pnpm --filter @SkyNft/nextjs vercel",
    "vercel:yolo": "pnpm --filter @SkyNft/nextjs vercel:yolo"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  }
}